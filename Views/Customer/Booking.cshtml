@model IEnumerable<Court_booking.ViewModel.BookingView>
@{
    ViewData["Title"] = "Booking";
}

<h2>Booking</h2>
@if (Model != null)
{
    <table class="table">
        <thead>
            <tr>
                <th>Date Time</th>
                <th>Court</th>
                <th></th>
            </tr>
        </thead>
        <tbody>

            @foreach (var b in Model)
            {
                <tr>
                    <td>
                        @b.booking.Book_time
                    </td>
                    <td>
                        @b.court_type
                    </td>
                    <td>
                        <a asp-action="DeleteBooking" asp-route-id="@b.booking.Id">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
@ViewBag.MaxMessage
<br />
@ViewBag.OverlapMessage
<br />
<a asp-action="AddBooking">Add</a>




